{
  "name": "stronk-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-transition-group": "^4.2.3",
    "@types/reactstrap": "^8.0.6",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "react-transition-group": "^4.3.0",
    "reactstrap": "^8.1.1",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/styles/index.pcss -o src/styles/output/index.css",
    "watch:css": "postcss src/styles/index.pcss -o src/styles/output/index.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "tailwindcss": "^1.1.2"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.config.js",
  "repository": {
    "type": "git",
